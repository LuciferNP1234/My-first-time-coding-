    <script>
        let count = 0;

        function submitFeedback(feeling) {
            count++;
            updateFeedbackCount();
            openPopup();
            // You can implement your logic to handle the feedback here
        }

        function updateFeedbackCount() {
            const countElement = document.getElementById("feedback-count");
            countElement.textContent = `${count} people have reacted.`;
        }

        function openPopup() {
            const popup = document.getElementById("feedback-popup");
            popup.style.display = "flex";
        }

        function closePopup() {
            const popup = document.getElementById("feedback-popup");
            popup.style.display = "none";
        }
    </script>


























